{
    "setupFilesAfterEnv": ["<rootDir>/__tests__/setupAfterEnv.jsx"],
    "setupFiles": ["<rootDir>/__tests__/setupEnv.jsx"],
    "transform": {
        "\\.([jt]sx|js|mjs)?$": "babel-jest"
    },
    "transformIgnorePatterns": ["\\.pnp\\.[^\\\/]+$"],
    "testPathIgnorePatterns": ["setupEnv.jsx", "setupAfterEnv.jsx"],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
        "pretendToBeVisual": true
    }
}